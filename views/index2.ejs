    var trim_Version=version[1].replace(/[ ]/g,"");
    if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE6.0")
    {
        alert("检测到您使用的是IE6浏览器，建议您更换IE8/9，谷歌浏览器或者火狐浏览器，已获得更好的浏览体验！");
    }
    else if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE7.0")
    {
        alert("检测到您使用的是IE7浏览器，建议您更换IE8/9，谷歌浏览器或者火狐浏览器，已获得更好的浏览体验！");
    }
    function showLogin(){
        $("#Mask").show();
        var LoginWidth=588;
        var Sceen=document.body.clientWidth;
        $("#Login").css("left",(Sceen-LoginWidth)/2+"px");
        $("#Login").show();
        window.scrollTo(0,0);
    }
    function showRegister(){
        $("#Mask").show();
        var LoginWidth=588;
        var Sceen=document.body.clientWidth;
        $("#Register").css("left",(Sceen-LoginWidth)/2+"px");
        $("#Register").show();
        window.scrollTo(0,0);

    }
    function ajaxLogin(){
        if (isLogin==false) {
            isLogin=true;
            var name=$("#login_name").val();
            var pwd=$("#login_pwd").val();
            if (name=="" || pwd=="")
            {
                alert("用户名和密码不能为空！");
            }
            else{
                $("#headerLogin").empty().append("<img style='width:16px; height:16px; margin:8px auto;' src='http://www.fjjj.com/assets/web/images/loadingSmall.gif'>");
                 $.ajax({
                    url: "/user/ajaxLogin",
                    type: "POST",
                    data: { cmd:name, Password:pwd },
                    success: function(data) {
                        if (data==0)
                        {
                            window.top.location.reload();
                        }
                        else if (data==2)
                        {
                            isLogin=false;
                            alert("用户不存在！");
                            $("#headerLogin").empty().text("登录");
                        }
                        else if (data==3)
                        {
                            isLogin=false;
                            alert("密码不正确");
                            $("#headerLogin").empty().text("登录");
                        }
                        else if (data==4)
                        {
                            self.location="/user/activating/"+name;
                        }
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                            alert("发生错误");
                            isLogin=false;
                    }
                });
            }

        };
    }
    function ajaxRegister(){
        if (isReg==false) {
            var name=$("#Register_name").val();
            var email=$("#Register_email").val();
            var pwd=$("#Register_pwd").val();
            var pwd2=$("#Register_pwd2").val();
            var telphone=$("#Register_telphone").val();
            var code=$("#Register_code").val();
            var agr=$("#agree").val();
            var myreg = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;
            if ($("#agree").is(':checked')==false)
            {
                alert("你必须接受服务条款！");

            }
            else if (name=="" || pwd=="" || email=="" || telphone=="" || code=="")
            {
                alert("用户名、电话、验证码、密码和邮箱不能为空！");
            }
            else if(name.match(/^[a-zA-Z0-9]*$/)==null){
                alert("用户名只能为大、小写字母和数字！请重新填写。");
            }
            else if(name.length<3 || name.length>18) {
                alert("请输入正确的用户名,用户名长度为3-18位！");
            }
            else if (pwd != pwd2)
            {
                alert("两次密码输入不一致，请重新填写。");
            }
            else if (pwd.length<6 || pwd.length>13 )
            {
                alert("请输入正确的密码,长度为6-13位！");
            }
            else if (!myreg.test(email))
            {
                alert('请输入有效的E_mail！');
            }
            else{
                isReg=true;
                var url=window.location.pathname;
                $("#headerRegister").empty().append("<img style='width:16px; height:16px; margin:8px auto;' src='http://www.fjjj.com/assets/web/images/loadingSmall.gif'>");
                 $.ajax({
                    url: "/user/ajaxRegister",
                    type: "POST",
                    data: { cmd:name, Password:pwd, email:email,url:url,telphone:telphone,code:code},
                    success: function(data) {
                        /*
                    　　if(data.indexOf('系统发生错误')!=-1)
                    　　{
                            $("#Register").append("<div style='display:none'>"+data+"</div>");
                            alert("注册已经成功，请登录后激活！!");
                            self.location="/user/login";
                    　　}*/
                    //alert(data);
                        if (data==1)
                        {

                            //alert("注册已经成功，请登录后激活！");
                            //self.location="/user/activating/"+name;
                            alert("注册已经成功，请登录");
                            self.location="/index";
                        }
                        else if (data==2)
                        {
                            alert("该用户名已存在！请选择其他用户名");
                        }
                        else if (data==3)
                        {
                            alert("该邮箱已经注册！请选择其他邮箱或者登录！");
                        }
                        else if (data==4)
                        {
                            alert("发生错误，请重新注册！");
                        }
                        else if (data==5)
                        {
                            alert("手机或验证码不正确！");
                        }
                        else if (data==6)
                        {
                            alert("该手机号已经注册！");
                        }
                        isReg=false;
                        //$("#headerRegister").empty().text("注册");
                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                            alert("发生错误");
                            isReg=false;
                    }
                });
            }

        };


    }
    function hideLogin(){
        $("#Mask").hide();
        $("#Login").hide();

    }
    function hideRegister(){
        $("#Mask").hide();
        $("#Register").hide();

    }
    function showUserAct(){
        $("#userMse").css("border","2px #367e97 solid");
        $("#userAct").show();
    }
    function hideUserAct(){
        $("#userMse").css("border","2px #fff solid");
        $("#userAct").hide();
    }
    function changeToLogin(){
        hideRegister();
        showLogin();
    }
    function changeToRegister(){
        hideLogin();
        showRegister();
    }
    function getCode(){
        var phone=$("#Register_telphone").val();
        if (phone=="") {
            alert("电话不能为空！");
        }
        else{
            if (IsTime==false) {
                if (IsGetCode==false) {
                    IsGetCode=true;
                    $("#time").show();
                    IsTime=true;
                    timeID3=setInterval(function(){
                        var t=Number($("#time").find(" span").text());
                        var t=t-1;
                        $("#time").find(" span").text(t);
                        if (t==0) {
                            clearInterval(timeID3);
                            IsTime=false;
                            $("#time").hide();
                            $("#time").find(" span").text(60);

                        }
                    },1000);
                    $.ajax({
                        url: "/index/getCode",
                        type: "POST",
                        data: {phone:phone},
                        success: function(data) {
                            if (data==1) {
                                alert("发送成功，请您查收！");
                                $("#time").hide();
                            }
                            else {
                                alert("发送失败，请重新发送！");
                                $("#time").hide();
                            }
                            IsGetCode=false;
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            alert("发生错误，请重试！")
                            $("#main").append("<div style='display:none'>ajaxError</div>");
                            IsGetCode=false;
                        }
                    });
                }
                else {
                    alert("您已经提交了验证，请耐心等待！");
                }
            }
            else {
                alert("一分钟只能提交一次！");
            }

        }

    }
</script>
